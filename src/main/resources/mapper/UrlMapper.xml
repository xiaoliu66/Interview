<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.baidu.mapper.UrlMapper">

    <select id="getUrlByRoleId" resultType="java.lang.Integer" parameterType="java.lang.Integer">
        select url_id from t_role_url where role_id = #{roleId}
    </select>
</mapper>